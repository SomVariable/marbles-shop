{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "style": "templates/default/TemplateName.module.scss",
        "index": "templates/default/index.ts"
      }
    },
    "module": {
      "path": "src/modules",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "style": "templates/module/styles/TemplateName.module.scss",
        "index": "templates/module/index.ts",
        "helpers": "templates/module/helpers/helpers.ts",
        "constats": "templates/module/constants/constants.ts"
      }
    }
  }
}